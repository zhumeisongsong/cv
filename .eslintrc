{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/prefer-stateless-function": [
      0
    ],
    "react/prop-types": 0,
    "prefer-template":0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "comma-dangle": ["error", "never"],
    "semi": ["error", "never"]
  }
}